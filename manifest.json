{
  "manifest_version": 2,
  "name": "Append Hostname to Title",
  "version": "1.0.3",
  "description": "Appends the hostname to every website's title.",
  "author": "Siddhartha Sahu",
  "homepage_url": "https://github.com/sdht0/append-hostname-to-title",
  "permissions": [
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"],
      "run_at": "document_end"
    }
  ]
}
